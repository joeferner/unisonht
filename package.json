{
  "name": "@unisonht/unisonht",
  "version": "0.0.2",
  "description": "UnisonHT - Unified Home Theatre",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "prepare": "rm -rf dist && npm run build",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --fix --ext .ts .",
    "lint-staged": "lint-staged",
    "pre-commit": "npm run format && npm run lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joeferner/unisonht.git"
  },
  "keywords": [
    "home",
    "theatre",
    "raspberry",
    "pi"
  ],
  "author": "Joe Ferner <joe@fernsroth.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joeferner/unisonht/issues"
  },
  "homepage": "https://github.com/joeferner/unisonht#readme",
  "dependencies": {
    "debug": "4.x",
    "path-to-regexp": "6.x"
  },
  "devDependencies": {
    "@types/debug": "4.x",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "4.x",
    "@typescript-eslint/parser": "4.x",
    "husky": "4.x",
    "lint-staged": "10.x",
    "prettier": "2.x",
    "source-map-loader": "1.x",
    "ts-loader": "8.x",
    "typescript": "4.x",
    "eslint": "7.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
