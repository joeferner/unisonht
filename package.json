{
  "name": "unisonht",
  "version": "1.0.0",
  "description": "UnisonHT server",
  "main": "dist/index.js",
  "bin": {
    "unisonht-server": "dist/bin/server.js"
  },
  "scripts": {
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start:watch": "DEBUG=unisonht:* concurrently \"tsc --watch\" \"ts-node-dev --respawn --transpile-only --ignore-watch public/ src/bin/server.ts --config sample.config.json\" \"nodemon --ext ts --ignore public/ -x tsoa spec\"",
    "swagger": "tsoa spec"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/joeferner/unisonht.git"
  },
  "keywords": [
    "unisonht",
    "home",
    "remote",
    "control"
  ],
  "author": "Joe Ferner <joe@fernsroth.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joeferner/unisonht/issues"
  },
  "homepage": "https://github.com/joeferner/unisonht#readme",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.24",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yargs": "^17.0.10",
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.15",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "express": "^4.17.3",
    "swagger-ui-express": "^4.3.0",
    "tsoa": "^3.14.1",
    "yargs": "^17.4.1"
  }
}